image boot.vfat {
	vfat {
		files = {
#BOOT_FILES#
		}
	}

	size = 32M
}

image sdcard.img {
	hdimage {
	}

	partition boot {
		partition-type = 0xC
		bootable = "true"
		image = "boot.vfat"
	}

	partition rootfs {
		partition-type = 0x83
		image = "rootfs.ext4"
	}

	partition ros2 {
		partition-type = 0x83
		image = "ros2.ext4"
	}
}
